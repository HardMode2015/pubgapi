{
    "name": "hardmode2015/pubgapi",
    "description": "A PUBG Developer API Wrapper for PHP and Laravel",
    "version": "0.2.6",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "ixudra/curl": "6.*",
        "pragmarx/ia-collection": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "hardmode2015\\pubgapi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hardmode2015\\pubgapi\\PubgApiServiceProvider"
            ] ,
            "aliases": {
                "PubgApi": "hardmode2015\\pubgapi\\Facades\\PubgApi"
            }
        }
    },
    "license": "MIT",
  
    "authors": [
